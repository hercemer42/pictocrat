{
  "name": "pictocrat",
  "version": "0.9.0",
  "description": "A picture viewer and manager",
  "homepage": "",
  "author": {
    "name": "Brian Fox",
    "email": "brian@brianfox.fr"
  },
  "keywords": [
    "picture",
    "image",
    "photo",
    "viewer",
    "manager"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "ng:serve:web": "ng serve -c web -o",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "npm run build:prod && electron-builder build --windows",
    "electron:mac": "npm run build:prod && electron-builder build --mac",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "testFront": "ng test",
    "watchFrontTests": "node_modules/onchange/dist/bin.js 'src/**/*.spec.ts' -- npm run testFront",
    "test": "npm run electron:serve-tsc && mocha --exit",
    "watchNodeTests": "node_modules/onchange/dist/bin.js 'test/**/*.js' 'lib/**/*.ts' -- npm run test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "watchE2eTests": "node_modules/onchange/dist/bin.js 'e2e/**/*.ts' 'src/**/*.ts' -- npm run e2e"
  },
  "dependencies": {
    "exifr": "6.0.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "nedb": "^1.8.0",
    "rimraf": "^2.6.2",
    "rxjs": "6.5.5",
    "rxjs-compat": "^6.0.0-rc.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "9.1.0",
    "@angular-devkit/build-angular": "0.901.3",
    "@angular-eslint/builder": "0.0.1-alpha.18",
    "@angular/cli": "9.1.3",
    "@angular/common": "9.1.3",
    "@angular/compiler": "9.1.3",
    "@angular/compiler-cli": "9.1.3",
    "@angular/core": "9.1.3",
    "@angular/forms": "9.1.3",
    "@angular/language-service": "9.1.3",
    "@angular/platform-browser": "9.1.3",
    "@angular/platform-browser-dynamic": "9.1.3",
    "@angular/router": "9.1.3",
    "@fortawesome/angular-fontawesome": "0.6.0",
    "@fortawesome/fontawesome-svg-core": "1.2.27",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "5.12.1",
    "@ngx-translate/core": "12.1.2",
    "@ngx-translate/http-loader": "4.0.0",
    "@types/chai": "4.2.11",
    "@types/chai-http": "4.2.0",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",
    "@types/mocha": "7.0.2",
    "@types/nedb": "1.8.9",
    "@types/node": "12.11.1",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "codelyzer": "5.2.2",
    "conventional-changelog-cli": "2.0.32",
    "core-js": "3.6.5",
    "cross-env": "7.0.2",
    "electron": "9.4.0",
    "electron-builder": "22.5.1",
    "electron-reload": "1.5.0",
    "eslint": "6.8.0",
    "eslint-plugin-import": "2.20.2",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.0.2",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-electron": "6.3.0",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.3",
    "karma-spec-reporter": "0.0.32",
    "mocha": "7.1.2",
    "npm-run-all": "4.1.5",
    "onchange": "7.0.2",
    "promise": "^8.0.1",
    "spectron": "10.0.1",
    "ts-node": "8.9.1",
    "tslib": "1.11.0",
    "typescript": "3.8.3",
    "wait-on": "4.0.2",
    "webdriver-manager": "12.1.7",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "zone.js": "0.10.3"
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
